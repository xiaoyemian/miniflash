<% include ../head4.ejs %>

<style>
body{
	overflow:hidden;
}
.header{
	height:72px;
}
.header .logo{
	float:left;
	height:72px;width:216px;
	background:url('image/logo.png') no-repeat center center;
}
.header .info{
	float:right;
	height:72px;width:216px;
	background:url('image/tzdbjk.png') no-repeat center center;
}
.main-nav{
	width:216px;
	position:absolute;
	left:0px;top:72px;bottom:0px;
}
.main-nav a{
	display:block;
	height:68px;
}
.body{
	position:absolute;
	top:72px;left:216px;
	right:0px; bottom:0px;
}
.main-content{
	height:100%;
	overflow:auto;
}
.second-nav{
	position:absolute;
	top:-40px;
	height:40px;
	overflow:hidden;
}
.second-nav a{
	float:left;
	width:140px;
	height:40px;
	background:url('image/2nava.png') no-repeat center center;
}
.second-nav a:hover{
	background:url('image/2navahover.png') no-repeat center center;
}

</style>



<div class="header">
	<div class="logo"></div>
	<div class="info"></div>
</div>

<div class="main-nav">
	<ul>
	<% 
		var mainList = [
			['shff', '手环发放']
			,['shjc','手环监控']
			,['jkda','健康档案']
			,['dbgl','达标管理']
			,['jkgl','健康管理']
			,['cqjl','出勤记录']
			,['kqgl','考勤管理']
			,['tzjc','体质监测']
			,['xtsz','系统设置']
			,['grsz','个人设置']
		]
		mainList.map(function(v,k){
	%>
		<style>
			.main-nav<%= k%> a{background:url('image/<%= v[1]%>1.jpg') no-repeat;}
			.main-nav<%= k%> a:hover{background:url('image/<%= v[1]%>2.jpg') no-repeat;}
			.main-nav<%= k%>.current a{background:url('image/<%= v[1]%>3.jpg') no-repeat;}
		</style>
		<li class="main-nav<%= k%> <%if(k==3){%>current<%}%>"><a href="/"></a></li>
	<%})%>
	</ul>

</div>

<div class="body">
	<div class="second-nav">
		<ul>
	<% 
			var mainList = [
				['yfsh', '已发手环']
				,['lssh','临时手环']
				,['dssh','丢失手环']
				,['kxsh','空闲手环']
				,['shsf','手环收发']
			]
			mainList.map(function(v,k){
		%>
			<li class="second-nav<%= k%><%if(k==3){%> current<%}%>"><a href="/"></a></li>
		<%})%>
		</ul>
	</div>

	<div class="main-content">
		<div style="height:1000px">123456</div>

	</div>
</div>


<script src="<%= JCSTATIC_BASE %>dist/loader.js" data-main="page/demo/35z"></script>
<script src="<%= JCSTATIC_BASE %>dist/page/demo/35z.js"></script>

